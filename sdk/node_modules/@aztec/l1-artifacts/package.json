{
  "name": "@aztec/l1-artifacts",
  "version": "4.0.0-devnet.2-patch.0",
  "type": "module",
  "exports": {
    "./*": "./dest/*.js",
    ".": "./dest/index.js"
  },
  "typedocOptions": {
    "entryPoints": "./generated/index.ts",
    "displayName": "L1 Contracts",
    "tsconfig": "./tsconfig.json"
  },
  "scripts": {
    "build": "yarn clean && yarn generate && ../scripts/tsc.sh",
    "clean": "rm -rf ./dest ./generated .tsbuildinfo",
    "formatting": "run -T prettier --check ./generated && run -T eslint ./generated",
    "formatting:fix": "run -T prettier -w ./generated",
    "copy-artifacts": "bash scripts/copy-foundry-artifacts.sh",
    "generate": "yarn copy-artifacts && yarn generate:l1-contracts",
    "generate:l1-contracts": "bash scripts/generate-artifacts.sh"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@typescript/native-preview": "7.0.0-dev.20260113.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dest",
    "generated",
    "l1-contracts"
  ],
  "types": "./dest/index.d.ts"
}
